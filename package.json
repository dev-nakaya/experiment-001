{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "markuplint": "markuplint \"src/**/*.vue\"",
    "markuplint:fix": "markuplint --fix \"src/**/*.vue\"",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run eslint:fix",
    "*.vue": [
      "npm run markuplint:fix",
      "npm run eslint:fix"
    ]
  },
  "dependencies": {
    "nuxt": "^3.11.2",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@markuplint/vue-parser": "^4.5.1",
    "@markuplint/vue-spec": "^4.4.1",
    "@nuxt/eslint": "^0.3.10",
    "@nuxt/test-utils": "^3.12.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@types/cheerio": "^0.22.35",
    "@types/highlight.js": "^10.1.0",
    "@vitest/ui": "^1.5.3",
    "@vue/test-utils": "^2.4.5",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "happy-dom": "^14.7.1",
    "highlight.js": "^11.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markuplint": "^4.6.1",
    "nuxt-icon": "^0.6.10",
    "playwright-core": "^1.43.1",
    "vite-plugin-eslint2": "^4.4.0",
    "vitest": "^1.5.3"
  }
}
