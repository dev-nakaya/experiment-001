{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "markuplint": "markuplint \"src/**/*.vue\"",
    "markuplint:fix": "markuplint --fix \"src/**/*.vue\"",
    "eslint": "eslint . --ignore-path .gitignore",
    "eslint:fix": "eslint . --fix --ignore-path .gitignore"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint:fix",
    "*.vue": [
      "markuplint:fix",
      "eslint:fix"
    ]
  },
  "dependencies": {
    "nuxt": "^3.11.2",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@markuplint/vue-parser": "^4.5.1",
    "@markuplint/vue-spec": "^4.4.1",
    "@nuxt/eslint": "^0.3.10",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markuplint": "^4.6.1",
    "vite-plugin-eslint2": "^4.4.0"
  }
}
